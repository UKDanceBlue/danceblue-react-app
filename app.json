{
  "expo": {
    "name": "UK DanceBlue",
    "slug": "danceblue-mobile",
    "privacy": "unlisted",
    "owner": "university-of-kentucky-danceblue",
    "scheme": "danceblue",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "platforms": ["ios", "android"],
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": ["assets/**"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.danceblue.app",
      "buildNumber": "1.3.0",
      "config": { "usesNonExemptEncryption": false },
      "infoPlist": {
        "NSRemindersUsageDescription": "Users may add DanceBlue events to their calendar."
      }
    },
    "android": {
      "package": "org.danceblue.app",
      "versionCode": 19,
      "permissions": ["WRITE_CALENDAR", "READ_CALENDAR"],
      "googleServicesFile": "./google-services.json"
    },
    "description": "University of Kentucky DanceBlue Mobile Application"
  }
}
