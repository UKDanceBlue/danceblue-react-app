{
  "expo": {
    "entryPoint": "./src/App.js",
    "name": "DanceBlue Mobile",
    "slug": "danceblue-mobile",
    "privacy": "unlisted",
    "owner": "university-of-kentucky-danceblue",
    "scheme": "danceblue",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "platforms": ["ios", "android"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danceblue.mobile",
      "buildNumber": "1.1.0"
    },
    "android": {
      "package": "com.danceblue.mobile",
      "versionCode": 5,
      "permissions": ["WRITE_CALENDAR", "READ_CALENDAR"],
      "googleServicesFile": "./google-services.json"
    },
    "description": "University of Kentucky DanceBlue Mobile Application",
    "sdkVersion": "43.0.0"
  }
}
