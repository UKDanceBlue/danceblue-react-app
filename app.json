{
  "expo": {
    "name": "UK DanceBlue",
    "slug": "danceblue-mobile",
    "privacy": "unlisted",
    "owner": "university-of-kentucky-danceblue",
    "scheme": "danceblue",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      "expo-calendar",
      "expo-camera",
      "expo-file-system",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to allow you to submit images for various DanceBlue activities.",
          "cameraPermission": "The app accesses your camera to allow you to submit images for various DanceBlue activities."
        }
      ],
      "expo-splash-screen",
      "expo-dev-client",
      "expo-updates",
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/notification-sound.wav",
            "./local/assets/notification-sound-other.wav"
          ]
        }
      ],
      "./config-plugins/rnf-disable-ai-id-support.plugin"
    ],
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "assets/**"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.danceblue.app",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSRemindersUsageDescription": "Users may add DanceBlue events to their calendar."
      },
      "associatedDomains": [
        "https://www.danceblue.org"
      ],
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "org.danceblue.app",
      "permissions": [
        "WRITE_CALENDAR",
        "READ_CALENDAR",
        "CAMERA"
      ],
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*.danceblue.org",
              "pathPrefix": "/redirect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "description": "University of Kentucky DanceBlue Mobile Application",
    "sdkVersion": "45.0.0"
  }
}
