{
    "tags": {
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src","config"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
      "destination": "doc",
      "recurse": true,
      "readme": "./README.md"
    }
  }