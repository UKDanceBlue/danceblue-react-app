{
  "cli": {
    "version": ">= 0.42.4",
    "requireCommit": true
  },
  "build": {
    "dev-client": {
      "distribution": "internal",
      "releaseChannel": "development",
      "developmentClient": true
    },
    "development": {
      "distribution": "internal",
      "releaseChannel": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "releaseChannel": "preview"
    },
    "production": {
      "distribution": "store",
      "releaseChannel": "prod-v1",
      "ios": {
        "autoIncrement": "buildNumber"
      },
      "android": {
        "autoIncrement": "versionCode"
      }
    }
  },
  "submit": {
    "dev-client": {
      "extends": "preview"
    },
    "development": {
      "extends": "preview"
    },
    "preview": {
      "extends": "production"
    },
    "production": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44"
      }
    }
  }
}
