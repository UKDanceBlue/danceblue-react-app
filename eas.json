{
  "cli": {
    "version": ">= 0.42.4",
    "requireCommit": true
  },
  "build": {
    "dev-client": {
      "distribution": "internal",
      "releaseChannel": "development",
      "developmentClient": true,
      "cache": {
        "key": "1"
      },
      "env": {
        "APP_VARIANT": "development"
      },
      "credentialsSource": "local"
    },
    "development": {
      "distribution": "internal",
      "releaseChannel": "development",
      "ios": {
        "simulator": true
      },
      "cache": {
        "key": "0"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "releaseChannel": "preview",
      "cache": {
        "key": "0"
      }
    },
    "production": {
      "distribution": "store",
      "releaseChannel": "prod-v2",
      "cache": {
        "key": "0"
      }
    }
  },
  "submit": {
    "dev-client": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44"
      }
    },
    "development": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44"
      }
    },
    "preview": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44"
      }
    },
    "production": {
      "ios": {
        "appleId": "app@danceblue.org",
        "appleTeamId": "622DXM8F44",
        "ascAppId": "1602033154"
      }
    }
  }
}
